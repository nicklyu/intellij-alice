<idea-plugin>
  <version>0.0.1</version>
  <change-notes>
      Add change notes here.&lt;br&gt;
      &lt;em&gt;most HTML tags may be used&lt;/em&gt;</change-notes>
  <idea-version since-build="183.4284" until-build="183.*"/>
  <id>intellij-voice.plugin</id>
  <name>Alice voice plugin</name>
  <vendor email="support@yourcompany.com" url="http://www.yourcompany.com">Nick Inc.</vendor>
  <description>
        Plugin provides additional way to interact with IDE
    </description>
  <extensions defaultExtensionNs="com.intellij">
    <projectService serviceInterface="org.intellij.alice.plugin.service.ActionCallService" serviceImplementation="org.intellij.alice.plugin.service.impl.DefaultActionCallService"/>
    <projectService serviceInterface="org.intellij.alice.plugin.service.AliceRemoteService" serviceImplementation="org.intellij.alice.plugin.service.impl.DefaultAliceRemoteService"/>
  </extensions>
  <actions/>
  <project-components>
    <component>
      <implementation-class>org.intellij.alice.plugin.component.AliceProjectComponent</implementation-class>
    </component>
  </project-components>
</idea-plugin>
